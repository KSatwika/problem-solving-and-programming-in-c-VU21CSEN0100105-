#include<stdio.h>
int main()
{
int a[10],n,i,large;

    printf("enter the range");
    scanf("%d",&n);
    printf("enter your elements");
    for(i=0;i<n;i++)       // 2 4 6 1 7 9
    {
     scanf("%d",&a[i]);   
    }
    large=0;
    for(i=0;i<n;i++)
    {
    if(a[i]>large)
    {
        large=a[i];
    }
    }
printf("max value is :%d",large);
return 0;
}
