include <stdio.h>

int main() {
char ch;
  
  printf("enter the letter");
  scanf("%c",&ch);
  if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
  {
      printf("vowels");
  }
  else
  {
     printf("consonants");
  }
          
     
    return 0;
}
output
enter the lettera
vowels
enter the letterb
consonants
