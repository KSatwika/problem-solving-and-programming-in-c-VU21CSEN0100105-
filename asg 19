#include<stdio.h>
int main()

{
char ch;
  printf("enter the value");
  scanf("%c",&ch);
    switch(ch)
    {
        case'-':
        printf("negative numbers");
        break;
        default:
    // ch doesn't match any case constant '-'
    
    printf("positive number");
     
    }
 return 0;
 }
 output
 enter the value-1
negative numbers
enter the value1
positive number
