#include <stdio.h>

int even(int a,int b)
{
    int i;
    for(i=a;i<b;i++)
    {
        if(a%2==0)
        {
            printf("\n%d",i++);
        }
        else
        {
            printf("\n%d",++i);
        }
    }
    
}
int odd(int a,int b)
{
    int i;
    for(i=a;i<b;i++)
    {
        if(a%2==0)
        {
            printf("\n%d",++i);
        }
        else
        {
            printf("\n%d",i++);
        }
    }
}
int power(int b,int e)
{
    if(b>e)
{
    printf("\n%d",b);
}
else
{
    printf("\n%d",e);
}
}
int max(int b,int e)
{
    if(b>e)
{
    printf("\n%d",b);
}
else
{
    printf("\n%d",e);
}
}
int main()
{
    int p,q,r,s,t,u;
    printf("enter p");
    scanf("\n%d",&p);
    printf("enter q");
    scanf("\n%d",&q);
    printf("\n even number");
    r=even(p,q);
    printf("\n odd numbers");
    s=odd(p,q);
    printf("\n power");
    t=power(p,q);
    printf("\n max");
    u=max(p,q);
}

