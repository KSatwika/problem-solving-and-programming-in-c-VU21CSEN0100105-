#include <stdio.h>

int main() {
  int n,i;
  unsigned long long fact=1;
  printf("enter an integer");
  scanf("%d",&n);
  if(n<0)
  {
      printf("error doesn't exist for negative numbers");
  }
  else
  {
      for(i=1;i<=n;i++)
      fact *=i;
      {
          
      }
      printf("factorial is %d=%llu",n,fact);
  }
  
    return 0;
}
output
enter an integer5
factorial is 5=120
