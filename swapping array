#include<stdio.h>
int main()
{
    int a[10],n,i,j,temp;
    printf("enter the array size");
    scanf("%d",&n);
    printf("enter the elemets");
    for(i=0;i<n;i++)
    {
        scanf("%d",&n);
    }
        for(i=0;i<n-1;j++)
        {
            for(j=0;j<n-1;j++)
            {
            
                if(a[j]>a[j+1])
                {
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
         }
        }
        printf("%d",a[i]);
    
    return 0;
}
